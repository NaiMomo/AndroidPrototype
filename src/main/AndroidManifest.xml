<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.reseau_social_ecole">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Reseau_Social_Ecole">
        <activity android:name=".ui.login.FriendReqActivity"></activity>
        <activity android:name=".ui.login.ProfileActivity" />
        <activity android:name=".ui.login.FriendsActivity" />
        <activity android:name=".ui.login.OtherProfileActivity" />
        <activity android:name=".ui.login.SearchResultActivity" />
        <activity android:name=".ui.login.PassForgotActivity" />
        <activity android:name=".ui.login.SignUpActivity" />
        <activity android:name=".ui.login.InterestActivity" />
        <activity android:name=".ui.login.MessageActivity" />
        <activity android:name=".ui.login.ChatActivity"/>
        <activity
            android:name=".ui.login.LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>